{
  "name": "wikicratie_projet",
  "version": "0.1.0",
  "description": "Wikicratie - Constitution Provisoire de Transition - Projet global",
  "scripts": {
    "startdev": "concurrently \"npm run serverdev\" \"npm run clientdev\"",
    "startdevwin": "concurrently \"npm run serverdevwin\" \"npm run clientdevwin\"",
    "start": "concurrently \"npm run server\" \"npm run client\"",
    "serverdev": "cd server && npm run dev",
    "serverdevwin": "cd server && npm run devwin",
    "server": "cd server && npm start",
    "client": "cd client && npm start",
    "clientdev": "cd client && npm run dev",
    "clientdevwin": "cd client && npm run devwin",
    "build": "cd client && npm run build && cd ../server && npm run build",
    "buildClient": "cd client && npm run build",
    "buildServer": "cd server && npm run build",
    "postinstall": "(cd server && npm install); (cd client && npm install)"
  },
  "engines": {
    "node": "12.x",
    "npm": "6.x"
  },
  "keywords": [
    "simulateur",
    "vaerdi",
    "ajnae",
    "investissement"
  ],
  "author": "Raphael ROBBE",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@types/react": "^17.0.0",
    "concurrently": "^5.0.2",
    "dompurify": "^2.4.0",
    "pg": "^7.18.2",
    "react-draft-wysiwyg": "^1.15.0",
    "typescript": "^4.5.5"
  },
  "resolutions": {
    "//": "See https://github.com/facebook/create-react-app/issues/11773",
    "react-error-overlay": "6.0.9"
  },
  "devDependencies": {
    "@types/dompurify": "^2.3.4",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.18.0",
    "draft-js": "^0.11.7",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-flowtype": "^4.6.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "prettier": "^2.0.0",
    "pretty-quick": "^3.1.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^1.12.0",
    "rollup-plugin-commonjs": "^10.1.0"
  }
}
